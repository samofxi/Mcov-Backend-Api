<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].Nachname %></td>
    <td><%= users[i].Vorname %></td>
    <td><%= users[i].date%></td>
    <td>
        <a href="/allResults?id=<%= users[i]._id %> " class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-envelope-open"></i></span>
        </a>
    </td>
</tr>
<% } %>